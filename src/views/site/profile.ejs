<!DOCTYPE html>
<html dir="ltr" lang="en">
  <%- include('./layouts/head'); -%>
<body>

    <div class="app">
        
      <%- include('./layouts/header'); -%>
        
        <div class="main-content">
            <div class="container" style="min-height: 100vh;">
                    <!-- Row -->
                    <div class="d-flex align-items-center justify-content-center m-5">
                        <div class="section-title mb-0">
                            <h2>Profile</h2>
                        </div>
                    </div>
                  
                    <div class="row shadow mb-4">
                      <div class="col-3 card">
                        <div class="profile-img m-5">
                          <img
                            src="https://scontent.fhan5-5.fna.fbcdn.net/v/t1.0-1/p200x200/95439104_3230333113857628_1109164386385133568_n.jpg?_nc_cat=101&amp;ccb=2&amp;_nc_sid=7206a8&amp;_nc_ohc=25XNtVDPiKEAX8sYn8C&amp;_nc_ht=scontent.fhan5-5.fna&amp;tp=6&amp;oh=ee84589e36cd6e5a7b8084e904810f13&amp;oe=5FFDEBA4"
                            alt="user" class="w-100 rounded-circle border border-success">
                        </div>
                        <div class="input-group">
                          <div class="custom-file m-auto">
                            <input type="file" class="custom-file-input" id="inputGroupFile04">
                            <label class="custom-file-label no-after" style="overflow: hidden;" for="inputGroupFile04">Choose image</label>
                          </div>
                          <div class="input-group-append">
                            <button class="btn btn-primary" type="button">Change Avatar</button>
                          </div>
                        </div>
                      </div>
                      <div class="col-9 card">
                        <form class="form-horizontal striped-rows b-form">
                          <div class="card-body">
                            <div class="form-group row border-bottom mb-0 py-3 bg-light align-items-center">
                              <div class="col-sm-3">
                                <div class="d-flex align-items-center justify-content-end">
                                  <label for="inputEmail3" class="control-label col-form-label">Full Name</label>
                                </div>
                              </div>
                              <div class="col-sm-9">
                                <input type="text" class="form-control" id="inputEmail3" placeholder="Full Name Here">
                              </div>
                            </div>
                            <div class="form-group row border-bottom mb-0 py-3 align-items-center">
                              <div class="col-sm-3">
                                <div class="d-flex align-items-center justify-content-end">
                                  <label for="inputEmail3" class="control-label col-form-label">Address</label>
                                </div>
                              </div>
                              <div class="col-sm-9">
                                <input type="text" class="form-control" id="inputEmail3" placeholder="Address Here">
                              </div>
                            </div>
                            <div class="form-group row border-bottom mb-0 py-3 bg-light align-items-center">
                              <div class="col-sm-3">
                                <div class="d-flex align-items-center justify-content-end">
                                  <label for="inputEmail3" class="control-label col-form-label">Company</label>
                                </div>
                              </div>
                              <div class="col-sm-9">
                                <input type="text" class="form-control" id="inputEmail3" placeholder="Company Name Here">
                              </div>
                            </div>
                            <div class="form-group row border-bottom mb-0 py-3 align-items-center">
                              <div class="col-sm-3">
                                <div class="d-flex align-items-center justify-content-end">
                                  <label for="inputEmail3" class="control-label col-form-label">Email</label>
                                </div>
                              </div>
                              <div class="col-sm-9">
                                <input type="email" class="form-control" id="inputEmail3" placeholder="Email Here">
                              </div>
                            </div>
                            <div class="form-group row border-bottom mb-0 py-3 bg-light align-items-center">
                              <div class="col-sm-3">
                                <div class="d-flex align-items-center justify-content-end">
                                  <label for="inputEmail3" class="control-label col-form-label">Phone Number</label>
                                </div>
                              </div>
                              <div class="col-sm-9">
                                <input type="text" class="form-control" id="inputEmail3" placeholder="Contact No Here">
                              </div>
                            </div>
                            <div class="form-group row mb-0 py-3 align-items-center">
                              <div class="col-sm-3">
                                <div class="d-flex align-items-center justify-content-end">
                                  <label for="inputEmail3" class="control-label col-form-label">Existing Customer</label>
                                </div>
                              </div>
                              <div class="col-sm-9">
                                <div class="custom-control custom-radio custom-control-inline mt-1">
                                  <input type="radio" class="custom-control-input" id="customControlValidation2" name="radio-stacked"
                                    required="">
                                  <label class="custom-control-label" for="customControlValidation2">Yes</label>
                                </div>
                                <div class="custom-control custom-radio custom-control-inline">
                                  <input type="radio" class="custom-control-input" id="customControlValidation3" name="radio-stacked"
                                    required="">
                                  <label class="custom-control-label" for="customControlValidation3">No</label>
                                </div>
                              </div>
                            </div>
                          </div>
                          <hr>
                          <div class="card-body">
                            <div class="form-group mb-0 text-right">
                              <button type="submit" class="btn btn-info waves-effect waves-light">Save</button>
                              <button type="submit" class="btn btn-dark waves-effect waves-light">Cancel</button>
                            </div>
                          </div>
                        </form>
                      </div>
                    </div>
            </div>
        </div>

        <%- include('./layouts/modal'); -%>
    
        <%- include('./layouts/footer'); -%>
</body>

</html>